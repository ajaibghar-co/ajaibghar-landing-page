---
const { ...props } = Astro.props;
import site from "../data/site.json";
---

<link rel="sitemap" href="/sitemap-index.xml" />
<title>{site.title}</title>
<meta name="description" content={site.bannercapabilities} />
<link
    rel="icon"
    type="image/svg+xml"
    href="/ajaibghar-landing-page/favicon.ico"
/>

<!-- Web analytics -->
<script
    data-goatcounter="https://my-account.goatcounter.com/count"
    async
    src="//gc.zgo.at/count.js"></script>

<!-- Open Graph tags -->
<meta property="og:title" content={site.title} />
<meta property="og:type" content="website" />
<meta property="og:url" content="http://www.ajaibghar.com/" />
<meta property="og:description" content={site.bannercapabilities} />
<meta
    property="og:image"
    content="https://www.example.com/_astro/seo-banner.BZD7kegZ.webp"
/>
<meta property="og:image:alt" content="" />

