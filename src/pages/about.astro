---
import "../styles/global.css";
import Layout from "../layouts/Layout.astro";
import Banner from "../components/Banner.astro";
import site from "../data/site.json";
const sentences = [
    "a studio for culture, stories & wonder",
    "a lab for people, play & exploration",
    "a lab for discovery, design & experimentation",
];
let sent;
let inter = () => {
    sent = Math.floor(Math.random() * sentences.length);
    return sentences[sent];
};

let t = setInterval(inter, 2500);
---

<Layout title="About">
    <Banner content1={inter} content2={site.about.content} client:load />
    <!-- <Banner content1={sent} content2={site.about.content} /> -->
</Layout>
